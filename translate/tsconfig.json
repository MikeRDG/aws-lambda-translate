{
    "compilerOptions": {
      "downlevelIteration": true,
      "strict": true,
      "preserveConstEnums": true,
      "noEmit": true,
      "sourceMap": false,
      "module":"es2015",
      "moduleResolution":"node",
      "esModuleInterop": true, 
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "target": "es2015",
      "lib": [
        "es2015",
        "dom"
      ]
    },
    "exclude": ["node_modules", "**/*.test.ts"]
  }